<h3>
	Input your song
</h3>
<form action='/' method='post'>
	Author: 
	<input name='artist'> <br>
	Song: 
	<input name='song'>  <br>
	<select action='add' name='action'>
		<option value='add'>Add song</option>
	 	<option value='find'>Find song</option>
	 	<option value='delete'>Delete song</option>
 	</select>
	<input type="submit" value='Choose selected option'/>
</form>
<h3>
	List of Songs
</h3>
	<ul>
		<% @list.each do |song| %>
		<li>
			<%= song.join("\n") %>
		</li>
		<% end %>
	</ul>